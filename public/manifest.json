
{
  "name": "Breaks",
  "manifest_version": 3,
  "version": "0.0.1",
  "description": "Retrieves headlines relevant to the website you are currently on",
  "content_scripts": [{
    "js": [ "content.js"],
    "matches": [ "<all_urls>"]
   }],
   "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
   },
   "permissions": ["contextMenus", "tabs", "scripting"]
  }